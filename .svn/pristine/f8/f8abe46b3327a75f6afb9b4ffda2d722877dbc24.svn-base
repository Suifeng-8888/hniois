package com.hniois.monitor.entity;

import com.hniois.common.page.Page;

/**
 * 其他操作
 */
public class Handle extends Page {
    //id,h_name,s_id,s_name,time,person,soil_id,bz
    private String  id;//主键
    private String p_id;//农产id
    private String p_name;//农产名称
    private String h_name;//操作名称 如浇水/裁剪/套袋/扎竹竿/上花粉等等
    private String s_id;//作物id
    private String s_name;//作物名称
    private String time;//操作时间
    private String person;//操作人
    private String soil_id;//土地编号
    private String bz;//备注

    @Override
    public String getId() {
        return id;
    }

    @Override
    public void setId(String id) {
        this.id = id;
    }

    public String getP_id() {
        return p_id;
    }

    public void setP_id(String p_id) {
        this.p_id = p_id;
    }

    public String getP_name() {
        return p_name;
    }

    public void setP_name(String p_name) {
        this.p_name = p_name;
