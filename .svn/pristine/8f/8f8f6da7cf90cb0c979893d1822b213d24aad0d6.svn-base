.pad-0{
    padding: 0
}
a{
    color: #333;
}
.navbar .navbar-toggle .icon-bar {
    background-color: #333;
}
.w-100{
    width: 100%;
}
.m-b-20{
    margin-bottom: 20px;
}
.m-b-40{
    margin-bottom: 40px;
}
.white{
    color: #fff;
}
.blue{
    color: #6091ce;
}
.navbar {
    margin-bottom: 0px;
}
.carousel-inner img{
    width: 100%
}
/* banner */
.jumbotron{
    height: 300px;
    background: url('../imgs/bg.png') ;
    background-size:100% 100%;

}
@media (max-width: 600px) {
    .jumbotron{
        height: 200px;
        margin-bottom: 20px
    }

}
.jumbotron p {
    font-size: 18px;
    text-shadow: 2px 2px 4px #333;
    margin-bottom: 5px;
}
.jum-box{
    position: relative;
}

.xs-text-top{
    padding-top:10%;
}
.xs-text{
    text-align: center;

    font-weight: bold!important;
}
/* 右-蔬菜 */
.banner-greens{
    position:absolute;
    right: 80px;
    top:-15px;
    width: 350px;
    height: 300px;
    background: url('../imgs/greens.png');
    background-size:100% 100%;
}
/* 搜索框 */
.search-box{
    display: block;
    margin-top: 30px;
}
/* 进入平台按钮 */
.item-info{
    display: block;
    margin-left: auto;
    margin-right: auto;
    text-align: center;
    max-width: 310px;
    min-width: 220px;
    margin-bottom: 15px;
    height:120px;
    background: #f4f4f4;
    border: 1px solid #d2d2d2;
    position: relative;
    border-radius: 5px;
    box-shadow: 2px 2px #f5f5f5;
}
.info-wrap{
    position: absolute;
    top:50%;
    left:50%;
    margin-top: -30px;
    margin-left: -95px;
    width: 190px;
}
.info-txt,.icon-img{
    float: left;
}
.info-txt{
    margin-right: 10px;
}
.txt-top{
    font-weight: 700;
    font-size: 16px;
}
.icon-img{
    width: 50px;
    height: 50px;
}
.icon-img img{
    width: 100%;
    height: 100%;
}
.click{
    width: 100%;
    color: #2486cc;
    text-align: right;
    display: block;
}
/* 进入平台按钮  end */

.new-title{
    height: 50px;
    width: 100%;
    background: #eee;
    border-bottom:2px solid #166ec0;
    border-radius: 5px 5px 0 0;
}
.new-title span.title{
    float: left;
    background: #166ec0;
    border-radius: 5px 5px 0 0;
    width: 80px;
    height: 50px;
    color: #fff;
    line-height: 50px;
    font-weight: 700;
    font-size: 16px;
    text-align: center;
}
.new-title span.more a{
    float: right;
    padding-right:10px;
    line-height: 50px;
    color:#166ec0;
}
/* 自定义地图 */
.map-box{
    height:400px ;
    width: 100%;
    border: 1px solid #333;
    color:aqua;
    font-size: 36px;
    line-height: 400px;
    text-align: center;
}
/* 自定义地图 end*/
/* 底部按钮块 */
.map-btn{
    background: #ebebeb;
    border: 1px solid #d2d2d2;
    text-align: center;
    line-height: 50px;
    display: block;
    margin-left: auto;
    margin-right: auto;
    text-align: center;
    box-shadow: 2px 2px #f5f5f5;
}
.map-btn p{
    line-height: 5px;
    color: #166ec0;
}
/* 底部 */
footer{
    width: 100%;
    padding:10px 0;
    background: #a0a0a0;
    overflow: hidden;
}
footer div.foot-info{
    display: block;
    width: 100%;
    text-align: center;
    font-weight: 600;
    color: #fff;
    line-height: 35px;
}
#map .anchorBL{
    display: none;
}
