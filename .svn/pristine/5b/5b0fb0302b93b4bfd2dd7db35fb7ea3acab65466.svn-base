            <if test="p_name!='' and p_name!=null">`p_name`=#{p_name},</if>
            <if test="p_form!='' and p_form!=null">`p_form`=#{p_form},</if>
            <if test="p_industry!='' and p_industry!=null">`p_industry`=#{p_industry},</if>
