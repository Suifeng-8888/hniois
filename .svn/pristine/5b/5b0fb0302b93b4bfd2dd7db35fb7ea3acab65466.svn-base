            <if test="p_latitude!='' and p_latitude!=null">`p_latitude`=#{p_latitude},</if>
            <if test="p_qualifications!='' and p_qualifications!=null">`p_qualifications`=#{p_qualifications},</if>
        </set>
        where p_id=#{p_id}
    </update>



