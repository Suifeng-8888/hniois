
body{
    background:url("../imgs/bbg.png") top center;
    width:100%;
    height:100%;
    position:absolute;
    z-index: 100;
    clear: both;
    /*overflow: hidden;*/
    /*filter : progid:DXImageTransform.Microsoft.AlphaImageLoader ( enabled=bEnabled , sizingMethod=sSize , src=sURL;*/
}
.topBar{
    background: url("../imgs/top.png") no-repeat top center;
    width: 100%;
    height: 130px;
    position:relative;
    z-index: 1000;
    overflow: hidden;
    clear: both;
}
.topBar h5{
    width: 100%;
    text-align: center;
    color: #00fffc;
    font-size: 45px;
    text-shadow:0px 0px 20px #04fdff;
    margin: 25px auto;

}
.topBar p{
    text-align: center;
    color: white;
    font-size: 14px;
    margin: -10px auto;
}

.topLT{
    background: url("../imgs/smBg.png") no-repeat center top;
    position:absolute;
    width:25%;
    height:28%;
    /*background-size:55%;*/
    background-size:100% 100%;
    float: left;
    margin: 0px 20px;

}
.topLT h5{
    padding-top: 10px;
    padding-bottom: 10px;
    width: 90%;
    font-size: 48px;
    color: #fe475d;
    font-weight: 600;
    margin: 10px;
    line-height: 20px;
    margin-left: 20px;
    clear: both;
}
.desc{
    float: left;
    margin-left: 20px;
    color: white;
    margin-top: -16px;
    font-size: 15px;
}
@media screen and (max-width:1280px){
    .topLT h5{
        padding-top: 2px;
        width: 90%;
        font-size: 22px;
        color: #fe475d;
        font-weight: 300;
        /*margin: 10px;*/
        /*line-height: 20px;*/
        margin-left: 20px;
        clear: both;
    }
    .desc{
        font-size: 12px;
        float: left;margin-left: 20px;color: white;margin-top: -20px
    }
}



@media screen and (min-width:1200px){
    .topLT h5 span{
        font-size: 15px;
        color: white;
        font-weight: 500;
    }
}

@media screen and (min-width: 400px) and (max-width: 1199px) {
    .topLT h5 span{
        font-size: 12px;
        color: white;
    }
}

.mainLT{
    background: url("../imgs/smBg.png") no-repeat center top;
    width: 25%;
    height: 23.7%;
    background-size: 100% 100%;
    float: left;
    margin-left: 20px;
    /*margin: 200px 40px;*/
    position: absolute;
    transform: translateY(128%);

}
.mainLT h5{
    margin-top: 10px;
    width: 90%;
    font-size: 15px;
    color: white;
    margin-left: 20px;
    position: absolute;
    z-index: 3000;
}
.mainLT h5 a{
    float: right;
    color: #02ffff;
    text-decoration: none;


}
.bottomLT{
    background: url("../imgs/smBg.png") no-repeat center top;
    width: 25%;
    height: 29.5%;
    background-size: 100% 100%;
    float: left;
    margin-left: 20px;
    position: absolute;
    transform: translateY(190%);

}
.bottomLT h5{
    font-size: 15px;
    color: white;
    margin-left: 20px;
    width: 90%;
}
.bottomLT h5 a{
    float: right;
    color: #02ffff;
    text-decoration: none;
}

.bottomLT .tit{
    margin-top: 10px;
    width: 100%;
    height: auto;
}
.bottomLT .tit a{
    text-align: center;
    text-decoration: none;
    color: #919295;
}
