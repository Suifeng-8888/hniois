.bottomLT{
    overflow: hidden;

}
.bottomLT .hd{
    overflow: hidden;
    width: 100%;
}
.bottomLT .hd div{
    width: 100%;

}
.bottomLT .hd div p{

    color: white;
    font-size: 14px;
    opacity: 0.5;
    float: left;
    overflow : hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp:1;
    -webkit-box-orient: vertical;
    cursor: pointer;
    text-align: center;
    height: 24px;
}
.lfBox{
    width: 100%;
    height: 62%;
    overflow: hidden;
    clear: both;
    /*margin-top: 10px;*/
}
.lfBox ul{
    width: 100%;
    height: 170px;
    overflow: hidden;
    clear: both;
    margin-bottom: 0px !important;


}
.lfBox ul li{
    width: 100%;
    height: 30px;
    overflow: hidden;
    margin-top: 1px;
    clear: both;
    line-height: 30px;

}
.lfBox ul li:hover{
    background: rgba(128,128,128,0.5);

}
.lfBox ul li:hover div p{

}
.lfBox ul li div{
    width: 100%;
    height: auto;

}
.lfBox ul li div p{
    color: white;
    font-size: 13px;
    float: left;
    overflow : hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp:1;
    -webkit-box-orient: vertical;
    cursor: pointer;
    text-align: center;
}
.bottomRT .hd{
