package com.hniois.monitor.entity;

import com.hniois.common.page.Page;

public class Harvest extends Page {
    //id,s_id,s_name,nums,person,h_time,batch,state,bz
    private String id;//主键  为导出excel表格使用
    private String farm_id;//农场主键
    private String soil_id;//土地编号
    private String s_id;//农作物id
    private String s_name;//农作物名称
    private String person;//采摘负责人
    private String h_time;//采摘时间
    private String batch;//采摘批次
    private String state;//是否已检验 0待检测 1已检测

    @Override
    public String getId() {
        return id;
    }

    @Override
    public void setId(String id) {
        this.id = id;
    }

    public String getSoil_id() {
        return soil_id;
    }

    public void setSoil_id(String soil_id) {
        this.soil_id = soil_id;
    }

    public String getFarm_id() {
        return farm_id;
    }

    public void setFarm_id(String farm_id) {
        this.farm_id = farm_id;
    }

    public String getS_id() {
        return s_id;
    }

    public void setS_id(String s_id) {
        this.s_id = s_id;
    }

    public String getS_name() {
        return s_name;
    }

    public void setS_name(String s_name) {
        this.s_name = s_name;
    }

    public String getPerson() {
        return person;
    }

    public void setPerson(String person) {
        this.person = person;
    }

    public String getH_time() {
        return h_time;
    }

    public void setH_time(String h_time) {
        this.h_time = h_time;
    }

    public String getBatch() {
        return batch;
    }

    public void setBatch(String batch) {
        this.batch = batch;
    }

    public String getState() {
        return state;
    }

    public void setState(String state) {
        this.state = state;
    }

}
